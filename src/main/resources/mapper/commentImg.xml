<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.idle.shoppingmall.mapper.CommentImgMapper">

    <insert id="saveCommentImg" parameterType="com.idle.shoppingmall.Entity.Comment_img" useGeneratedKeys="true" keyProperty="img_id">
        INSERT INTO comment_img (comment_id, img_url, img_name)
        VALUES (#{commentImg.comment_id}, #{commentImg.img_url}, #{commentImg.img_name})
    </insert>

</mapper>